<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebApplication2</title>
    <link rel="stylesheet" href="~/css/metro-all.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" /> 
</head>
<body>
    <header>
        <nav data-role="appbar" data-expand-point="md" class="app-bar app-bar-expand">
            <a href="#" class="brand no-hover">
                <span style=" " class=" ">
                    m<sup>4</sup>
                </span>
            </a>
            <ul class="app-bar-menu">
                <li><a href="#">Home</a></li>
                <li>
                    <a href="#" class="dropdown-toggle">Products</a>
                    <ul class="d-menu" data-role="dropdown">
                        <li><a href="#">Windows 10</a></li>
                        <li><a href="#">Office 365</a></li>
                        <li class="divider bg-lightGray"></li>
                        <li><a href="#">Skype</a></li>
                    </ul>
                </li>
                <li> <a  asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                <li><a   asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a></li>
                <partial name="_LoginPartial" />
            </ul>
 
        </nav>
    </header>
    <div class="maincontainer">
        <main role="main" class=" ">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - WebApplication2 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/js/metro.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
